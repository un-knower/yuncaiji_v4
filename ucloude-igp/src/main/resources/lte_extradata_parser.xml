<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="cellCfgInfoParser" class="cn.uway.framework.parser.DatabaseParser" scope="prototype">
		<property name="templates">
			<value>parser/lte/extradata/L_CELL_CFG.xml</value>
		</property>
	</bean>
	
	<bean name="neicellCfgInfoParser" class="cn.uway.framework.parser.DatabaseParser" scope="prototype">
		<property name="templates">
			<value>parser/lte/extradata/L_NEI_CELL_CFG.xml</value>
		</property>
	</bean>
	
	<bean name="neicellCfgSimpleInfoParser" class="cn.uway.framework.parser.DatabaseParser" scope="prototype">
		<property name="templates">
			<value>parser/lte/extradata/L_NEI_CELL_CFG_SIMPLE.xml</value>
		</property>
	</bean>
</beans>